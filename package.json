{
  "name": "appc.odata",
  "description": "OData connector",
  "version": "1.1.0",
  "author": "Vladimir Trifonov",
  "license": "",
  "framework": "none",
  "keywords": [
    "appcelerator",
    "arrow"
  ],
  "repository": {},
  "private": true,
  "dependencies": {
    "appc-connector-utils": "0.0.9",
    "async": "2.1.5",
    "fast-xml-parser": "2.3.0",
    "joi": "10.2.2",
    "mixin-deep": "1.2.0",
    "npm": "4.3.0",
    "odata": "0.3.0",
    "ramda": "0.23.0",
    "request": "2.81.0",
    "semver": "5.3.0"
  },
  "devDependencies": {
    "arrow": "^*",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.0.0",
    "babel-istanbul": "^0.12.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-stage-2": "^6.22.0",
    "eslint": "3.17.1",
    "eslint-config-standard": "7.0.1",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "2.1.1",
    "mockery": "2.0.0",
    "nsp": "2.6.3",
    "nyc": "10.1.2",
    "retire": "1.2.12",
    "rimraf": "2.6.1",
    "sinon": "1.17.7",
    "sinon-stub-promise": "4.0.0",
    "standard": "9.0.1",
    "tap": "10.3.0",
    "tap-mocha-reporter": "3.0.3"
  },
  "scripts": {
    "start": "appc run",
    "build": "npm run clean && npm run build:dev && npm run security",
    "build:dev": "npm run build:dist && npm run build:utils",
    "build:dist": "babel -d ./dist ./lib",
    "build:utils": "babel -d ./dist ./utils",
    "lint": "standard --fix",
    "clean": "rimraf ./tmp && rimraf dist/* coverage/*",
    "test:coverage": "nyc check-coverage --lines 90 --functions 95 --branches 85 --statements 90",
    "test": "npm run security && npm run test:unit && npm run clean",
    "test:unit": "tap ./test/unit/* --color --bail --coverage --coverage-report=html && npm run test:coverage",
    "test:integration": "tap ./test/integration/* --color --bail",
    "security": "retire --exitwith 0 --outputpath scan-retire.json && nsp check --output json > scan-nsp.json"  
  }
}
